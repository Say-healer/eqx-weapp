<view bindtap="handleTap" class="eqx-class eqx-cell {{ isLastCell ? 'eqx-cell-last' : '' }} {{ isLink ? 'eqx-cell-access' : '' }}">
    <view class="eqx-cell-icon">
        <slot name="icon"></slot>
    </view>
    <view class="eqx-cell-bd">
        <view wx:if="{{ title }}" class="eqx-cell-text">{{ title }}</view>
        <view wx:if="{{ label }}" class="eqx-cell-desc">{{ label }}</view>
        <slot></slot>
    </view>
    <view catchtap="navigateTo" class="eqx-cell-ft">
        <block wx:if="{{value}}">{{ value }}</block>
        <block wx:else>
            <slot name="footer"></slot>
        </block>
    </view>
</view>